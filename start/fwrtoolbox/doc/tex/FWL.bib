	Journal = {IEEE Transactions on Circuits and Systems},
	Keywords = {Optimal Design, Roundoff Noise},
	Local-Url = {file://localhost/Users/Thib/Recherche/biblio/Optimal%20Realizations(according%20to%20a%20special%20measure)/%5BRote95%5D%20Optimal%20Realizations%20of%20Finite%20Wordlength%20Digital%20Filters%20and%20Controllers.pdf},
